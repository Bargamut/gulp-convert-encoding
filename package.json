{
  "name": "gulp-convert-encoding",
  "version": "2.0.1",
  "description": "Convert files from one encoding to another using iconv-lite.",
  "license": "MIT",
  "repository": "heldinz/gulp-convert-encoding",
  "author": {
    "name": "Alice Rose",
    "email": "heldinz@posteo.de",
    "url": "https://github.com/heldinz"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "test": "mocha -R spec",
    "coverage": "istanbul cover node_modules/.bin/_mocha --report html -- -R spec",
    "coveralls": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "iconv",
    "iconv-lite",
    "convert",
    "encoding",
    "decoding",
    "charset"
  ],
  "dependencies": {
    "iconv-lite": "0.4.24",
    "plugin-error": "1.0.1",
    "through2": "2.0.5"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "event-stream": "3.3.4",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "should": "13.2.3",
    "vinyl": "2.2.0"
  }
}
